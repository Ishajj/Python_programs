   "source": [
    "def find_substring(st, sub):\n",
    "    \"find substring in a string\"\n",
    "    if sub in st:\n",
    "        x=len(sub)\n",
    "        return st.index(sub[0:1+x])             \n",
    "            \n",
    "    else:\n",
    "        return -1"
   ]
